					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"method": "GET",
					"url": {
						"value": "@concat('https://', variables('key_vault_subdomain'), '.vault.azure.net/secrets/anz-gpg-public-key?api-version=7.0')",
